<Window x:Class="har1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:har1"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Frame></Frame>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button x:Name="PlayButton" Content="Play" Width="200" Margin="5" Click="PlayButton_Click"/>
            <Button x:Name="HighscoreButton" Content="Highscores" Width="200" Margin="5"/>
            <Button x:Name="InstructionsButton" Content="Instructions" Width="200" Margin="5"/>
        </StackPanel>
    </Grid>
</Window>
